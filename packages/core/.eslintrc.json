{
  "extends": ["plugin:es/restrict-to-es2018"],
  "env": {
    "browser": true
  },
  "plugins": ["@typescript-eslint"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "ignorePatterns": ["src/test/**/*", "dist/**/*", "coverage/**/*"]
}
